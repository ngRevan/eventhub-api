{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(local);Initial Catalog=EventHub;Integrated Security=SSPI;MultipleActiveResultSets=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "EventHub": {
        "Profile": "SPA",
        "RedirectUri": "https://localhost:4200/signin-oidc",
        "LogoutUri": "https://localhost:4200/signout-callback-oidc"
      }
    }
  },
  "AllowedHosts": "*"
}
